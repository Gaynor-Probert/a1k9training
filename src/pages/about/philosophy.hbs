{{#extend "layout"}}

    {{#content "intro"}}
        {{#if model.components.intro}}
            {{> (lookup model.components 'intro')}}
        {{/if}}
    {{/content}}


    {{#content "main"}}
        <div class="page-header">
            <h1>{{model.title}}</h1>
        </div>

        <div class="row">
            <div class="col-md-12">
                
                <img src="/images/about/offlead-run-v1.png" class="img-responsive img-thumbnail pull-right"
                     style="margin-left: 10px;" alt=""/>

                {{#if model.components.content}}
                    {{> (lookup model.components 'content')}}
                {{/if}}

                <a href="{{ model.next.link }}" class="btn btn-primary">Next: {{ model.next.name}} </a>

            </div>
        </div>
    {{/content}}

{{/extend}}
