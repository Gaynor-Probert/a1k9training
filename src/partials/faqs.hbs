<div class="row">
  <div class="page-header"><h4>Dynamic - Frequently Asked Questions</h4></div>
  <div class="panel-group" id="accordion">
    {{#each this}}
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" data-parent="#accordion" href="#collapse_{{@index}}">
            <strong>Q{{offset @index}}:</strong>
            {{q}}
          </a>
        </h4>
      </div>
      <div id="collapse_{{@index}}" class="panel-collapse collapse">
        <div class="panel-body">
          <span class="label label-info">Answer:</span>
          {{a}}
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>
